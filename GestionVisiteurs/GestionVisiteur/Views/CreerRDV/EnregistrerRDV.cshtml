
@{
    ViewBag.Title = "EnregistrerRDV";
}



<!--Affiche les données-->
<div class="card m-4">
    <div style="background-color:#008080; color:#ffffff" class="card-header">
        <div class="row">
            <div class="form-group col-lg-4 col-sm-4 col-xs-11 col-md-9 col-xl-8">
                <i class="fa fa-user"></i>Liste des visites
            </div>
            <div class="form-group col-lg-8 col-sm-8 col-xs-1 col-md-3 col-xl-4">
                <div class="input-group">
                    <div class="form-outline">
                        <input type="search" id="form1" class="form-control" placeholder="recherche" />
                    </div>
                    <button type="button" class="btn btn-primary">
                        <i class="fas fa-search"></i>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered table-hover">
                <tr>
                    
                    <th>Date</th>
                    <th>Visiteur</th>                 
                    <th>Motif</th>
                    <th>Heure d'entrée</th>                    
                    <th>Actions</th>
                </tr>
                <tr>                                    
                  
                    <th>...</th>
                    <th>...</th>
                    <th>...</th>
                    <th>...</th>
                    <th style="width:auto">
                        <a class="btn btn-labeled btn-warning btn-sm modifier" href=" ">
                            <span class="btn-label"> <i class="fa fa-edit"></i> </span>modifier
                        </a>

                        <a class="btn btn-labeled btn-info btn-sm detail" href=" ">
                            <span class="btn-label"> <i class="fa fa-info"></i> </span>Detail
                        </a>

                        <a class="btn btn-labeled btn-danger btn-sm supprimer" href=" ">
                            <span class="btn-label"> <i class="fa fa-trash"></i> </span>Supprime
                        </a>


                    </th>
                </tr>
            </table>
        </div>
    </div>
</div>
